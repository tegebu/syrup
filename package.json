{
  "name": "@tegebu/syrup",
  "version": "1.0.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:tegebu/syrup.git"
  },
  "license": "MIT",
  "author": "tegebu <info@tegebu.com>",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn clean && concurrently \"npm:build:*\"",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "clean": "rimraf dist && rimraf coverage",
    "commit": "git cz",
    "dev": "yarn start",
    "format": "eslint --fix 'src/**/*.{ts,tsx}'",
    "prepublish": "yarn build",
    "release": "semantic-release",
    "test": "jest test --detectOpenHandles --coverage",
    "update": "yarn update:upgrade && yarn update:install",
    "update:install": "yarn install && yarn upgrade",
    "update:upgrade": "ncu --upgrade"
  },
  "dependencies": {
    "@jamashita/publikum-collection": "2.25.0",
    "@jamashita/publikum-error": "2.25.0",
    "@jamashita/publikum-interface": "2.25.0",
    "@jamashita/publikum-json": "2.25.0",
    "@jamashita/publikum-object": "2.25.0",
    "@jamashita/publikum-type": "2.25.0",
    "@jamashita/publikum-uuid": "2.25.0",
    "@jamashita/publikum-zeit": "2.25.0"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@jamashita/eslint-config": "1.2.2",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.1.1",
    "@semantic-release/npm": "7.0.6",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/jest": "26.0.14",
    "@types/node": "14.11.2",
    "@types/sinon": "9.0.8",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "@typescript-eslint/typescript-estree": "4.3.0",
    "concurrently": "5.3.0",
    "eslint": "7.10.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.0.2",
    "git-cz": "4.7.1",
    "husky": "4.3.0",
    "jest": "26.5.0",
    "lint-staged": "10.4.0",
    "npm-check-updates": "9.0.3",
    "rimraf": "3.0.2",
    "semantic-release": "17.1.2",
    "sinon": "9.1.0",
    "sort-package-json": "1.46.0",
    "ts-jest": "26.4.1",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  },
  "engines": {
    "node": ">= 14.5.0",
    "npm": "npm is not supported"
  },
  "engineStrict": true
}
