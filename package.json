{
  "name": "@tegebu/syrup",
  "version": "1.7.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:tegebu/syrup.git"
  },
  "license": "MIT",
  "author": "tegebu <info@tegebu.com>",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn clean && concurrently \"npm:build:*\"",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "clean": "rimraf dist && rimraf coverage",
    "commit": "git cz",
    "dev": "yarn start",
    "format": "eslint --fix 'src/**/*.{ts,tsx}'",
    "prepublish": "yarn build",
    "release": "semantic-release",
    "test": "jest test",
    "update": "yarn update:upgrade && yarn update:install",
    "update:install": "yarn install && yarn upgrade",
    "update:upgrade": "ncu --upgrade"
  },
  "dependencies": {
    "@jamashita/publikum-collection": "3.4.0",
    "@jamashita/publikum-error": "3.4.0",
    "@jamashita/publikum-interface": "3.4.0",
    "@jamashita/publikum-json": "3.4.0",
    "@jamashita/publikum-object": "3.4.0",
    "@jamashita/publikum-tree": "3.4.0",
    "@jamashita/publikum-type": "3.4.0",
    "@jamashita/publikum-uuid": "3.4.0",
    "@jamashita/publikum-validation": "3.4.0",
    "@jamashita/publikum-zeit": "3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@jamashita/eslint-config": "1.11.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.0",
    "@semantic-release/npm": "7.0.9",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/jest": "26.0.16",
    "@types/node": "14.14.10",
    "@types/sinon": "9.0.9",
    "@typescript-eslint/eslint-plugin": "4.9.0",
    "@typescript-eslint/parser": "4.9.0",
    "@typescript-eslint/typescript-estree": "4.9.0",
    "concurrently": "5.3.0",
    "eslint": "7.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "git-cz": "4.7.5",
    "husky": "4.3.4",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "npm-check-updates": "10.2.2",
    "rimraf": "3.0.2",
    "semantic-release": "17.3.0",
    "sinon": "9.2.1",
    "sort-package-json": "1.48.0",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.0",
    "typescript": "4.1.2"
  },
  "engines": {
    "node": ">= 14.5.0",
    "npm": "npm is not supported"
  },
  "engineStrict": true
}
